// Hierarchical Model
digraph {
	P [label=Price]
	S [label=SqFt]
	B [label=Bedrooms]
	Ba [label=Bathrooms]
	O [label=Offers]
	N [label=Neighborhood]
	Br [label=Brick]
	B -> P [label="β2"]
	Ba -> P [label="β3"]
	O -> P [label="β4"]
	S -> P [label="β1"]
	N -> P [label="u0, u1"]
	Br -> P [label="u0, u1"]
}
